#include <iostream>
using namespace std;
int main() {
  int password;
    cout<<"Enter the passward"<<endl;
    cin>>password;
    while (password<999999)
    {
cout<<"The password you enter is not correct, please enter it again"<<endl;
      cin>>password;
    }
    cout<<"The password is correct"<<endl;
}



#include<iostream>
