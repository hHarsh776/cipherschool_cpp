#include <iostream>
using namespace std;
void PrintHi()
{
    cout<<"Hi"<<endl;
    return;
}

int sum(int a, int b)
{
    int c;
    c = a+b;
    return c;
}
void swap(int a,int b)
{
    int c;
    c=a;
    a=b;
    b=c;
cout<<"The value of a and b after swapping inside the function"<<a<<" "<<b<<endl;
    return;
}

int main()
{
    PrintHi();
    int a,b;
    a=4;
    b=6;
    
    // int d;
    // d=sum(a,b);
    
    //cout<<"The sum is equal to"" "<<d<<" "<<"The value of a is"" "<<a;
swap(a,b);
cout<<"The value of a and b after swapping ouside the function"<<a<<" "<<b;

    return 0;
}
